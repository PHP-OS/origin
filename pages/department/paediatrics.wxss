page{
  background:#eeeeee;
}
.container{
  width:100%;
  padding:0;
  margin:0;
  background:#eeeeee;
}
.calendar{
  width:100%;
  height:150rpx;
  background:#ffffff;
  overflow: hidden;
  margin:20rpx auto;
  font-size:30rpx;
  color: #4497f7;
}
.cal-con{
  width:90rpx;
  height:90rpx;
  text-align: center;
  margin:30rpx 30rpx;
  float:left;
}
.item{
  width:100%;
  height:240rpx;
  margin-bottom: 20rpx;
  background:#ffffff;
  overflow: hidden;
}
.imgInfo{
  width:160rpx;
  height:170rpx;
  margin:20rpx;
  margin-top:30rpx;
  float:left;
}
.con{
  width:100%;
  height:180rpx;
  padding-top:20rpx;
  font-size:24rpx;
}
.con-top{
  overflow: hidden;
  vertical-align: bottom;
}
.left{
  font-size: 32rpx;
  display: inline-block;
  margin-right:10rpx;
   vertical-align: bottom;
}
.blue{
  color:#4497f7;
  vertical-align: bottom;
  display: inline-block;
}
.gray{
  color:#999999;
}
.gray-top{
  overflow: hidden;
}
.gray-left{
  float:left;
  margin-right:30rpx;
}
.footer{
  overflow: hidden;
}
.btn{
  width:145rpx;
  line-height:50rpx;
  margin-top:20rpx;
  margin-right:30rpx;
  float:left;
  font-size:22rpx;
  color:#fff;
  background:#4497f7;
}
.btn1{
  background:#f4652b;
}
.btn2{
  background:#0eb750;
}
