.container {
  background-color: #f3f3f3;
  min-height: 100%;
  display: block;
}

.mask{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    background: #000;
    z-index: 9000;
    opacity: 0.7;
}
.modalDlg{
    width: 463rpx;
    height: 583rpx;
    position: fixed;
    top: 50%;
    left: 9%;
    z-index: 9999;
    margin: -370rpx 85rpx;
    border-radius: 20rpx;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.big{
  position: fixed;
  top:22%;
  left:39%;
  text-align: center;
  font-size:46rpx;
  color:#ffffff;
}
.modal-btn{
  width:341rpx;
  border-radius: 45rpx;
  line-height:65rpx;
  color:#fff;
  font-size: 28rpx;
  background:#4497F7;
  position: fixed;
  top: 60%;
  left: 28%;
  z-index: 9999;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.modelgo{
    width:100rpx;
    position: fixed;
    top: 75%;
    left: 45%;
    z-index: 9999;
    display: flex;
    flex-direction: column;
    align-items: center;
}
.go{
  width:50rpx;
  height:50rpx;
  border:1rpx solid #ccc;
  border-radius: 50rpx;
}

.swiper {
  height: 300rpx;
  width: 100%;
  
} 
swiper-item .slide-image{
  width: 100%;
} 

.list-kind{
  padding: 20rpx 0;
}
.icon-view{
  background-color: #fff;
  border-radius: 15rpx;
  box-shadow: 5rpx  10rpx 10rpx #ddd;
  overflow: hidden;
}
.kinds{
  width: 20%;
  text-align: center;
  display: inline-block;
  overflow: hidden;
}
.kinds-box{
  width: 100%;
  margin-top: 20px;
  overflow: hidden;
}
.kinds-box image{
  width: 80rpx;
  height: 80rpx;
  
}
.kinds-box .kind{
  font-size: 12px;
  text-align: center;
}
.hybanner{
  background-color: #fff;
  height: 220rpx;
  overflow: hidden;
  padding: 20rpx;
  margin-bottom: 20rpx;
}
.hybanner-inner{
  width: 100%;
  overflow: hidden;
  height: 100%;
   
}
.hybanner-inner image{
  width: 100%;
  height: 100%;
}
.service{
  padding: 20rpx;
  background-color: #fff;
  margin-bottom: 20rpx;
}
.service-title{
  padding-top: 0;
  padding-bottom: 0;
  height: 50rpx;
  margin-bottom: 20rpx;
  vertical-align: middle;
  overflow: hidden;
  position: relative;
}
.service-title .bor-left{
  display: inline-block;
  width: 6rpx;
  background-color: #297ce4;
  height: 36rpx;
  margin-right: 10rpx;
  vertical-align: middle;
}
.bor-right{
   display: inline-block;
   vertical-align: middle;
   font-weight: bold;  
   font-size: 34rpx;
   color: #333;
}
.service-title .more{
  display: inline-block;
  font-size: 22rpx;
  color: #297ce4;
  position: absolute;  
  bottom:0;
  right: 20rpx;
}
.ser-items{
  padding: 20epx;
}
.ser-items-top{
  overflow: hidden;
  margin-bottom: 20rpx;
}
.ser-item-l,.ser-item-r{
  width: 48%;
  display: inline-block;
  box-shadow: 0 10rpx  10rpx  #ddd;
  height: 300rpx;
  border-radius: 15rpx;
  overflow: hidden;
}
.ser-item-l {
  margin-right: 2%;
}
.ser-item-r {
  margin-left: 2%;
}
.ser-item-l image,.ser-item-r image{
  width: 100%;
  height: 100%;
}
.ser-items-bottom{
  overflow: hidden;  
}
.bot-item-l,.bot-item-m,.bot-item-r{
  width: 30%;
  display: inline-block;
 box-shadow: 0 10rpx  10rpx  #ddd;
  height: 300rpx;
  border-radius: 15rpx;
  overflow: hidden;

}
.bot-item-l image,.bot-item-m image,.bot-item-r image{
  width: 100%;
  height: 100%;
}
.bot-item-m{
  margin-left: 5%;
  margin-right: 5%;
}
.baike-list{
  border-radius: 15rpx;
  box-shadow:  0 10rpx  10rpx  #ddd;
  margin-bottom: 20rpx;
}
.baike-image {
  width: 100%;
  height: 250rpx;
}
.baike-image image{
  width: 100%;
  height: 100%;
}
.baike-text{
  padding: 20rpx;
  font-size: 34rpx;
  letter-spacing: 2rpx;
  color: #666;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.youxuan-items{
  overflow: hidden;
}
.youxuan-item{
  width: 50%;
  display: inline-block;
  margin-bottom: 30rpx;
}
.youxuan-item-inner{
  border-radius: 15rpx;
  box-shadow:  0 10rpx  10rpx  #ddd;
  width: 90%;
  margin: 0 auto;
}
.yx-text{
  padding: 10rpx 20rpx;
  position: relative;
}
.yx-text .title{
  font-size: 30rpx;
  color: #666;
}

.chart .chart-l{
  display: inline-block;
  color: red;
  font-size: 30rpx;
  font-weight: bold;
}
.chart .chart-r{
  display: inline-block;
  width: 50rpx;
  position: absolute;
  bottom: 5rpx;
  right: 20rpx;
}

.chart-r image{
  width: 50rpx;
  height: 40rpx;
}