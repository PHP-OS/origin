page{
  background:#eeeeee;
}
.container{
  width:100%;
  padding:0;
  margin:0;
  background:#eeeeee;
  font-size:30rpx;
  color:#999999;
}
.header{
  width:100%;
  height:129rpx;
  background:#4497f7;
  overflow: hidden;
}
.head-left{
  width:16rpx;
  height:30rpx;
  margin-top:80rpx;
  margin-left:30rpx;
  float:left;
}
.head-title{
  color:#fff;
  margin-top:70rpx;
  margin-left:315rpx;
}
.con{
  width:100%;
  overflow: hidden;
}
.time-sec{
  overflow: hidden;
  width:330rpx;
  height:60rpx;
  background:#fff;
  float:left;
  margin-left:30rpx;
  margin-top:20rpx;
  border:1rpx solid #ccc;
}
.adds-sec{
  overflow: hidden;
  width:330rpx;
  height:60rpx;
  background:#fff;
  float:right;
  margin-right:30rpx;
  margin-top:20rpx;
  border:1rpx solid #ccc;
}
.time{
  margin:5rpx 0 0 20rpx;
  color:#000;
  float:left;
}
.select{
  width:30rpx;
  height:16rpx;
  float:right;
  margin:20rpx 20rpx;
}
.address{
  margin:5rpx 90rpx 0 20rpx;
  color:#000;
  float:left;
}
.selection{
  overflow: hidden;
}
.name{
  width:690rpx;
  line-height:90rpx;
  margin:20rpx auto 0;
  background:#fff;
  text-align: center;
  color: #333333;
  font-size:30rpx;
  border-bottom: 1rpx solid #ccc;
}
.item{
  width:690rpx;
  line-height:90rpx;
  margin:0 auto;
  background:#fff;
  text-align: center;
  color: #333333;
  font-size:30rpx;
  overflow: hidden;
}
.item1{
  width:690rpx;
  line-height:90rpx;
  margin:20rpx auto 0;
  background:#fff;
  text-align: center;
  color: #333333;
  font-size:30rpx;
  overflow: hidden;
}
.item-left{
  float:left;
  color:#666666;
  margin:0 20rpx;
}
.item-right{
  width:250rpx;
  float:right;
}
.item-con{
  color:#666666;
  float:right;
}
.select1{
  width:40rpx;
  height:26rpx;
  float:right;
  margin:37rpx 20rpx;
}